{
    "rules": {
        "class-name": false,
        "trailing-comma": [
            false,
            {
                "multiline": "always",
                "singleline": "never"
            }
        ],
        "interface-name": [false, "always-prefix"],
        "only-arrow-functions": false,
        "no-console": [false,
            "time",
            "timeEnd",
            "trace"
        ],
        "max-line-length": [
            true,
            {
                "limit": 120,
                "ignore-pattern": "//"
            }
        ],
        "triple-equals": [
            true,
            "allow-null-check"
        ],
        "ordered-imports": false,
        "import-spacing": true,
        "prefer-const": true,
        "quotemark": [
            true,
            "single",
            "avoid-escape"
        ],
        "variable-name": [
            false,
            "allow-leading-underscore",
            "allow-trailing-underscore",
            "allow-pascal-case",
            "ban-keywords",
            "check-format"
        ],
        "member-ordering": [
            false,
            {
                "order": [
                    "static-field",
                    "instance-field",
                    "static-method",
                    "instance-method"
                ]
            }
        ],
        "indent": [
            true,
            "spaces"
        ],
        "one-line": [
            true,
            "check-open-brace",
            "check-catch",
            "check-else",
            "check-whitespace"
        ],
        "no-var-keyword": true,
        "semicolon": [
            true,
            "always"
        ],
        "whitespace": [
            true,
            "check-branch",
            "check-operator",
            "check-module",
            "check-separator",
            "check-type"
        ],
        "typedef-whitespace": [
            true,
            {
                "call-signature": "nospace",
                "index-signature": "nospace",
                "parameter": "nospace",
                "property-declaration": "nospace",
                "variable-declaration": "nospace"
            },
            {
                "call-signature": "onespace",
                "index-signature": "onespace",
                "parameter": "onespace",
                "property-declaration": "onespace",
                "variable-declaration": "onespace"
            }
        ],
        "comment-format": [
            true,
            "check-space"
        ],
        "deprecation": {
            "severity": "warn"
        },
        "no-internal-module": true,
        "no-empty": false,
        "no-empty-interface": false,
        "no-trailing-whitespace": true,
        "no-switch-case-fall-through": true,
        "no-shadowed-variable": true,
        "no-redundant-jsdoc": true,
        "no-misused-new": true,
        "no-string-literal": false,
        "no-use-before-declare": true,
        "no-unused-expression": true,
        "no-unnecessary-initializer": true,
        "object-literal-sort-keys": false,
        "jsdoc-format": true
    }
}
